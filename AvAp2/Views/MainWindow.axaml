<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:AvAp2.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="AvAp2.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="AvAp2"
	xmlns:models="using:AvAp2.Models">

    <Design.DataContext>
        <vm:MainWindowViewModel/>
    </Design.DataContext>
	<Grid>
    
	<models:CAutomaticSwitch x:Name="CAutomaticSwitch1" Width="300" Height="200" ></models:CAutomaticSwitch>
		<models:CCellCart x:Name="CCellCart1" Width="100" Height="100" ></models:CCellCart>
	<Button x:Name="AutomaticSwitchButton" Height="30" Width="180" Margin="50" > Изменить состояние</Button>
	</Grid>
</Window>
