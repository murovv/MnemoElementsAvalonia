<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:AvAp2.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="AvAp2.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="AvAp2"
	xmlns:models="using:AvAp2.Models">
	<Window.Styles>
		<StyleInclude Source="/Views/CCellCart2Styles.axaml" />
	</Window.Styles>
    <Design.DataContext>
        <vm:MainWindowViewModel/>
    </Design.DataContext>
	<Grid>
    <UniformGrid Rows="2" Columns="3">
	    <models:CAutomaticSwitch x:Name="CAutomaticSwitch1" Margin="30"></models:CAutomaticSwitch>
	    <models:CCellCart x:Name="CCellCart1" ></models:CCellCart>
	    <models:CCellCart2 x:Name="CCellCart2"></models:CCellCart2>
	    <Button x:Name="AutomaticSwitchButton" Margin="30" Height="30"> Изменить выключатель</Button>
	    <Button x:Name="CellCartButton"  Height="30"> Изменить выкатной элемент</Button>
	    <Button x:Name="CellCart2Button"  Height="30"> Изменить выкатной элемент 2</Button>
    </UniformGrid>
	
		
	</Grid>
</Window>
