<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:PanAndZoomTesting.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:PanAndZoomTesting.Views"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="PanAndZoomTesting.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="PanAndZoomTesting">

    <Design.DataContext>
        <vm:MainWindowViewModel />
    </Design.DataContext>
    <Window.Styles>
        <StyleInclude Source="/Styles/BasicMnemoElementStyles.axaml"></StyleInclude>
        <StyleInclude Source="/Styles/BasicWithTextNameStyles.axaml"></StyleInclude>
        <StyleInclude Source="/Styles/BasicWithStateStyles.axaml"></StyleInclude>
        <StyleInclude Source="/Styles/BasicCommutationDeviceStyles.axaml"></StyleInclude>
        <StyleInclude Source="/Styles/CLineStyles.axaml" />
        <StyleInclude Source="/Styles/IGeometryStyles.axaml"></StyleInclude>
    </Window.Styles>
    <views:ZoomPanel></views:ZoomPanel>
</Window>