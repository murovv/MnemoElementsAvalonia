<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:models="clr-namespace:AvAp2.Models"
        xmlns:models1="clr-namespace:AvAp2.Models;assembly=AvAp2"
        x:DataType="models1:BasicMnemoElement">
    <Design.PreviewWith>
        <Border Padding="20">
            <!-- Add Controls for Previewer Here -->
        </Border>
    </Design.PreviewWith>
    <Style Selector=":is(models1|BasicMnemoElement)">
        <Setter Property="Content">
            <Template>
                <Canvas ClipToBounds="False">
                    <ContentControl Content="{CompiledBinding DrawingIsSelectedWrapper}"></ContentControl>
                    
                </Canvas>
            </Template>
        </Setter>
    </Style>
    <Style Selector=":is(models1|BasicMnemoElement):pointerover">
        <Setter Property="Content">
            <Template>
                <Canvas ClipToBounds="False">
                    <ContentControl Content="{CompiledBinding DrawingIsSelectedWrapper}">
                    </ContentControl>
                    <ContentControl Content="{CompiledBinding DrawingMouseOverWrapper}"></ContentControl>
                </Canvas>
            </Template>
        </Setter>
    </Style>

    <!-- Add Styles Here -->
</Styles>
